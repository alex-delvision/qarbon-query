{
  "name": "@qarbon/emissions",
  "version": "1.0.0",
  "exports": {
    ".": "./dist/index.js",
    "./calculator": "./dist/calculator.js",
    "./factors": "./dist/factors.js",
    "./compatibility": "./dist/compatibility.js"
  },
  "description": "Carbon emissions calculation engine and methodologies for QarbonQuery",
  "repository": {
    "type": "git",
    "url": "https://github.com/qarbon/qarbon-query.git",
    "directory": "packages/@qarbon/emissions"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:browser": "vitest run --browser",
    "test:ai": "vitest run --grep='ai|emission|gpt'",
    "clean": "rm -rf dist",
    "docs": "typedoc --skipErrorChecking --excludeExternals --out docs src/calculator.ts src/adapters/index.ts src/factors.ts"
  },
  "dependencies": {
    "@qarbon/shared": "file:../shared"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@vitest/browser": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "playwright": "^1.40.0",
    "semantic-release": "^24.2.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
