{
  "name": "@qarbon/emissions",
  "version": "0.3.0",
  "description": "Carbon emissions calculation engine and methodologies for QarbonQuery",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ai": "jest --testNamePattern='ai|emission|gpt'",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@qarbon/shared": "file:../shared"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
