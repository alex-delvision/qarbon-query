{
  "name": "@qarbon/shared",
  "version": "0.1.0",
  "description": "Shared types, utilities, and constants for QarbonQuery ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {},
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delvision/qarbon-query.git",
    "directory": "packages/@qarbon/shared"
  },
  "keywords": [
    "carbon",
    "emissions",
    "sustainability",
    "types",
    "utilities"
  ]
}
